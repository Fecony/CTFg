{
    "name": "ctfg",
    "private": true,
    "version": "0.0.1",
    "module": "true",
    "scripts": {
        "build": "NODE_ENV=production rollup -c",
        "start": "NODE_ENV=development rollup -c -w",
        "gen:proto": "protoc -I ../proto --plugin=protoc-gen-ts=\"./node_modules/.bin/protoc-gen-ts\" --plugin=protoc-gen-twirp_ts=\"./node_modules/.bin/protoc-gen-twirp_ts\" --ts_opt=client_none --ts_opt=generate_dependencies --ts_out=src/rpc --twirp_ts_out=src/rpc ctfg.proto",
        "validate": "svelte-check"
    },
    "devDependencies": {
        "@protobuf-ts/plugin": "^2.2.3-alpha.1",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@rollup/plugin-typescript": "^8.1.0",
        "@tsconfig/svelte": "^1.0.0",
        "@types/node": "^18.11.19",
        "autoprefixer": "^10.2.3",
        "fastify-plugin": "^4.5.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-dev": "^2.0.4",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.38.2",
        "svelte-check": "^1.0.0",
        "svelte-preprocess": "^5.0.1",
        "tslib": "^2.0.0",
        "twirp-ts-og": "npm:twirp-ts",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "rollup-plugin-polyfill-node": "^0.12.0",
        "svelte-routing": "^1.6.0",
        "svelvet": "^5.0.8",
        "twirp-ts": "git+https://github.com/breadchris/twirp-ts.git#3b136df46886a13eaa45ebc1fedbc996b760bbff"
    }
}
